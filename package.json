{
  "name": "alakajam",
  "version": "2.0.0-dev",
  "description": "Website powering the Alakajam! game making community",
  "license": "MIT",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node ./node_modules/ts-node-dev/bin/ts-node-dev -r tsconfig-paths/register server/index.ts",
    "start:refresh": "concurrently --kill-others \"npm run build:watch\" \"browser-refresh\"",
    "start:e2e": "node ./node_modules/ts-node-dev/bin/ts-node-dev -r tsconfig-paths/register server/e2e.ts",
    "start:debug": "tsc && node --nolazy -r ts-node/register -r tsconfig-paths/register --inspect-brk dist/server/index.js",
    "build:css": "node -r ts-node/register server/sass.ts",
    "build:client": "node -r ts-node/register server/webpack.ts",
    "build:server": "tsc --build tsconfig.build.json",
    "deployment:build": "npm run build:css && npm run build:client && npm run build:server",
    "deployment:start": "node dist/server/index.js",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "tslint --project tsconfig.json --fix",
    "lint:client": "tslint --project client/tsconfig.json --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "mocha -r ts-node/register -r tsconfig-paths/register server/**/*.spec.ts --exit",
    "test:e2e": "start-server-and-test start:e2e http://localhost:8001 cypress:run",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "knex": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/knex/bin/cli.js --knexfile server/core/knexfile.ts",
    "migrate:latest": "npm run knex -- migrate:latest",
    "migrate:rollback": "npm run knex -- migrate:rollback",
    "migrate:currentVersion": "npm run knex -- migrate:currentVersion",
    "docs": "typedoc --out dist/docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alakajam-team/alakajam.git"
  },
  "bugs": {
    "url": "https://github.com/alakajam-team/alakajam/issues"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "bookshelf": "^0.14.2",
    "bookshelf-cascade-delete": "^2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^3.0.1",
    "connect-session-knex": "^1.4.0",
    "cookies": "^0.7.3",
    "copy": "^0.3.2",
    "core-js": "^3.0.0",
    "csurf": "^1.9.0",
    "download": "^6.2.5",
    "express": "^4.16.4",
    "express-nunjucks": "^2.2.3",
    "express-promise-router": "^1.1.1",
    "express-session": "^1.15.6",
    "find-up": "^3.0.0",
    "flipclock": "^0.7.8",
    "html-to-text": "^3.3.0",
    "knex": "^0.16.3",
    "left-pad": "^1.3.0",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.22",
    "mkdirp": "^0.5.1",
    "module-alias": "^2.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "multer": "^1.4.1",
    "node-cache": "^4.2.0",
    "nodemailer": "^4.7.0",
    "npm": "^6.9.0",
    "null-loader": "^0.1.1",
