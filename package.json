    "@types/luxon": "^1.15.2",
    "@types/mocha": "^7.0.1",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.7.0",
    "@types/nodemailer": "^6.2.2",
    "@types/nunjucks": "^3.1.2",
    "@types/sass": "^1.16.0",
    "@types/showdown": "^1.9.3",
    "@types/sinon": "^7.5.0",
    "@types/slug": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "bootstrap": "^4.4.1",
    "browser-refresh": "^1.7.3",
    "browser-refresh-client": "^1.1.4",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
