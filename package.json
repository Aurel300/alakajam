{
  "name": "alakajam",
  "version": "2.0.0-dev",
  "description": "Website powering the Alakajam! game making community",
  "license": "MIT",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules server/index.ts",
    "start:refresh": "concurrently --kill-others \"npm run build:server -- -w\" \"browser-refresh\"",
    "start:e2e": "ts-node -r tsconfig-paths/register server/e2e.ts",
    "start:debug": "tsc && node --nolazy -r ts-node/register -r tsconfig-paths/register --inspect-brk dist/server/index.js",
    "start:production": "node dist/server/index.js",
    "build": "npm run build:css && npm run build:client && npm run build:server",
    "build:css": "node -r ts-node/register server/sass.ts",
    "build:sass": "node -r ts-node/register server/sass.ts",
    "build:client": "node -r ts-node/register server/webpack.ts",
    "build:server": "tsc --build tsconfig.build.json",
    "lint": "npm run lint:server && npm run lint:client && npm run lint:e2e",
    "lint:server": "eslint server/ --ext .ts --fix",
    "lint:client": "eslint client/ --ext .ts --fix",
    "lint:e2e": "eslint cypress/ --ext .ts --fix",
    "test": "npm run test:unit:ci && npm run test:e2e",
    "test:unit": "mocha -r ts-node/register -r tsconfig-paths/register server/**/*.spec.ts --watch --watch-extensions ts --watch-files server",
    "test:unit:ci": "mocha -r tsconfig-paths/register dist/server/**/*.spec.js --exit",
    "test:e2e": "start-server-and-test start:e2e http://localhost:8001 cypress:run",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "knex": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/knex/bin/cli.js --knexfile server/core/knexfile.ts",
    "migrate:latest": "node deployment/verify-data-folder.js && npm run knex -- migrate:latest",
    "migrate:rollback": "npm run knex -- migrate:rollback",
    "migrate:currentVersion": "npm run knex -- migrate:currentVersion",
    "docs": "typedoc --out dist/docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alakajam-team/alakajam.git"
  },
  "bugs": {
    "url": "https://github.com/alakajam-team/alakajam/issues"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@ttskch/select2-bootstrap4-theme": "^1.3.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "bookshelf": "^0.15.1",
