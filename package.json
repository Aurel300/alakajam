    "@types/bookshelf": "^0.13.3",
    "@types/chai": "^4.2.8",
    "@types/copy": "^0.3.1",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.15",
