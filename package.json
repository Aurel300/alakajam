    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.18.2",
    "bookshelf": "^0.10.2",
    "bookshelf-cascade-delete": "^2.0.1",
    "cookies": "^0.7.0",
    "express": "^4.14.0",
    "express-nunjucks": "^2.2.3",
    "express-promise-router": "^1.1.1",
    "html-to-text": "^3.3.0",
    "knex": "^0.12.6",
    "lodash": "^4.17.5",
    "mime-types": "^2.1.16",
