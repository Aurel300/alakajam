    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "node-cache": "^5.1.0",
    "nodemailer": "^6.3.1",
    "npm": "^6.13.7",
    "null-loader": "^3.0.0",
    "nunjucks": "^3.1.7",
    "pg": "^7.18.1",
    "popper.js": "^1.16.1",
    "random-key": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sanitize-html": "^1.21.1",
    "sass": "^1.25.0",
    "select2": "^4.0.13",
    "showdown": "^1.9.0",
    "slug": "^2.1.1",
    "sortablejs": "^1.10.1",
    "sqlite3": "^4.1.0",
