{
  "name": "alakajam",
  "version": "2.0.0-dev",
  "description": "Website powering the Alakajam! game making community",
  "license": "MIT",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "ts-node-dev --transpile-only -r tsconfig-paths/register --ignore-watch node_modules --ignore-watch \".*.tsx\" server/index.ts",
    "start:refresh": "npm run build:server && concurrently --kill-others \"npm run build:server -- -w\" \"browser-refresh\"",
    "start:e2e": "ts-node --transpile-only -r tsconfig-paths/register server/e2e.ts",
    "start:debug": "tsc && ts-node --transpile-only --nolazy -r tsconfig-paths/register --inspect-brk dist/server/index.js",
    "start:production": "node dist/server/index.js",
    "build": "npm run build:css && npm run build:client && npm run build:server",
    "build:css": "ts-node --transpile-only server/sass.ts",
    "build:sass": "ts-node --transpile-only server/sass.ts",
    "build:client": "ts-node --transpile-only server/webpack.ts",
    "build:server": "tsc --build tsconfig.build.json",
    "lint": "npm run lint:server && npm run lint:client && npm run lint:e2e",
    "lint:server": "eslint server/ --ext .ts,.tsx --fix",
    "lint:client": "eslint client/ --ext .ts,.tsx --fix",
    "lint:e2e": "eslint cypress/ --ext .ts --fix",
    "test": "npm run test:unit:ci && npm run test:e2e",
    "test:unit": "ts-node-dev --transpile-only -r tsconfig-paths/register server/mocha.ts",
    "test:unit:ci": "mocha -r ts-node/register/transpile-only -r tsconfig-paths/register server/**/*.spec.ts --exit",
    "test:e2e": "start-server-and-test start:e2e http://localhost:8001 cypress:run",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "knex": "ts-node --transpile-only -r tsconfig-paths/register ./node_modules/knex/bin/cli.js --knexfile server/core/knexfile.ts",
    "migrate:latest": "node deployment/verify-data-folder.js && npm run knex -- migrate:latest",
    "migrate:rollback": "npm run knex -- migrate:rollback",
    "migrate:currentVersion": "npm run knex -- migrate:currentVersion",
    "docs": "typedoc --out dist/docs --exclude **/*.spec.ts,**/*.controller.ts,**/migrations/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alakajam-team/alakajam.git"
  },
  "bugs": {
    "url": "https://github.com/alakajam-team/alakajam/issues"
  },
