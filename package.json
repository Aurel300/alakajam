    "optional-require": "^1.0.0",
    "pg": "^6.1.2",
    "promisify-node": "^0.5.0",
    "random-key": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "remove-markdown": "^0.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sanitize-html": "^1.20.0",
    "sass": "^1.21.0",
    "showdown": "^1.9.0",
    "slug": "^0.9.3",
    "sortablejs": "^1.8.1",
    "sqlite3": "^4.0.6",
    "striptags": "^3.0.1",
    "turndown": "^4.0.2",
    "typescript": "^3.4.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "validator": "^7.0.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.3.0",
